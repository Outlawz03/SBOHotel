		var bind = e.getTarget(),
			parentName = String(bind.getParentNode().getAttribute("Name")),
			name = String(bind.getAttribute("Name"));
			
		if(!_.has(this.binds, parentName)) {
			this.binds[parentName] = {};
		}
		
		if(!_.has(this.binds[parentName], name)) {
			this.binds[parentName][name] = Object.create(MYAPI.Bind);
		}
	},
	
	click: function(e) {
//		for(var key in this.binds) {
//			if(this.binds.hasOwnProperty(key)) {
//				var bind = this.bind[key],
//					updates = false;				
//				if(bind.container === container) {
//					updates = true;
//				}
//				bind.setUpdates(updates);
//			}
//		}
		var bind = e.getTarget(),
			parent = bind.getParentNode(),
			name = String(bind.getAttribute("Name")),
			parentName = String(parent.getAttribute("Name"));
			
		switch(name) {
			case "Higher":
				break;
			case "Lower":
				break;
			case "Room":
				this.binds[parentName]["NOTE1"].displayProperties();
				break;
			default:
		}
	}
};

/**==========================================================================================
 * SptCtrl Object
 */
MYAPI.SptCtrl = {
	init: function(node) {
		this.node = node;
		this.values = {
			"EffectiveOcc"			: {value: 0, units: null},
			"SystemMode"			: {value: 0, units: null},
			"PICoolDemand"			: {value: 0, units: null},
			"PIHeatDemand"			: {value: 0, units: null},
			"UnoccCoolSetpoint"		: {value: 0, units: null},
			"UnoccHeatSetpoint"		: {value: 0, units: null},
			"StandbyCoolSetpoint"	: {value: 0, units: null},
			"StandbyHeatSetpoint"	: {value: 0, units: null},
			"OccCoolSetpoint"		: {value: 0, units: null},
			"OccHeatSetpoint"		: {value: 0, units: null}
		};

		this.txt = Object.create(MYAPI.Text);
		this.txt.init(this.node);
	},
	update: function(e) {
		var name = String(e.getTarget().getAttribute("Name"));
		if(_.has(this.values, name)) {		
			this.values[name].value = +e.getValue();
			this.values[name].units = String(e.getUnit());
		
			var content = "";
			switch(this.values["EffectiveOcc"].value) {
			case 1:
			case 3:
				if(this.values["PIHeatDemand"].value > 0 || this.values["SystemMode"].value === 4) {
					content = this.values["OccHeatSetpoint"].value.toFixed(1) + " " + this.values["OccHeatSetpoint"].units;
				} else {
					content = this.values["OccCoolSetpoint"].value.toFixed(1) + " " + this.values["OccCoolSetpoint"].units;
				}
				break;
			case 2:
				if(this.values["PIHeatDemand"].value > 0  || this.values["SystemMode"].value === 4) {
					content = this.values["UnoccHeatSetpoint"].value.toFixed(1) + " " + this.values["UnoccHeatSetpoint"].units;
				} else {
					content = this.values["UnoccCoolSetpoint"].value.toFixed(1) + " " + this.values["UnoccCoolSetpoint"].units;
				}
				break;
			case 4:
				if(this.values["PIHeatDemand"].value > 0  || this.values["SystemMode"].value === 4) {
					content = this.values["StandbyHeatSetpoint"].value.toFixed(1) + " " + this.values["StandbyHeatSetpoint"].units;
				} else {
					content = this.values["StandbyCoolSetpoint"].value.toFixed(1) + " " + this.values["StandbyCoolSetpoint"].units;
				}
				break;
			default:
				content = "---°F";
			}
	
			this.txt.setContent(content);
		}
	}
};

/**==========================================================================================
 * OccCtrl Object
 */
MYAPI.OccCtrl = {
	init: function(node) {
		this.node = node;
		this.values = {
			"LowBattAlarm"	: 0,
			"EffectiveOcc"	: 0,
			"OccCommand"	: 0,
		};
		
		this.fills = ["#FFD100", "#339999", "#87D300", "#4FA600", "#42B4E6"];
		this.strokes = ["#404040", "#FFFFFF", "#404040", "#FFFFFF", "#FFFFFF"];
		this.contents = ["Low Battery", "Rented/Occupied", "Unrented/Unoccupied", "Unrented/Occupied", "Rented/Unoccupied"];

		this.bgd = Object.create(MYAPI.Rectangle);
		this.bgd.init(this.node.getChildByName("background"));
		
		this.fgd = Object.create(MYAPI.Rectangle);
		this.fgd.init(this.node.getChildByName("innerline"));
		
		this.txt = Object.create(MYAPI.Text);
		this.txt.init(this.node.getChildByName("Text"));		
	},
	update: function(e) {
		var name = String(e.getTarget().getAttribute("Name"));
		if(_.has(this.values, name)) {		
			this.values[name] = +e.getValue();
		
			var content = "";
			var fill = "#404040";
			var stroke = "#FFFFFF";
			if(this.values["LowBattAlarm"] === 1) {
				fill = this.fills[0];
				content = this.contents[0];
				stroke = this.strokes[0];
			} else {
				fill = this.fills[this.values["EffectiveOcc"]];
				content = this.contents[this.values["EffectiveOcc"]];
				stroke = this.strokes[this.values["EffectiveOcc"]];
			}
	
			this.txt.setContent(content);
			this.txt.setStroke(stroke);
			this.bgd.setFill(fill);
			this.fgd.setFill(fill);
		}
	}
};

/**==========================================================================================
 * Header Object
 */
MYAPI.HeaderCtrl = {
	init: function(node) {
		this.values = {
			"LowBattAlarm"	: 0,
			"EffectiveOcc"	: 0,
			"OccCommand"	: 0,
		};
		
		this.fills = ["#FFD100", "#339999", "#87D300", "#4FA600", "#42B4E6"];
		
		this.bgd = Object.create(MYAPI.Rectangle);
		this.bgd.init(node);		
	},
	update: function(e) {
		var name = String(e.getTarget().getAttribute("Name"));
		if(_.has(this.values, name)) {		
			this.values[name] = +e.getValue();
		
			var fill = "#404040";
			if(this.values["LowBattAlarm"] === 1) {
				fill = this.fills[0];
			} else {
				fill = this.fills[this.values["EffectiveOcc"]];
			}
			this.bgd.setFill(fill);
		}
	}
};

