                    <Script OnDocumentLoad="onLoad"
                        OnMouseClick="onClick" OnSignalChange="onChange"><![CDATA[var comp;
var swAni;
var bgAni;
var binds;
var rCol;
var lCol;
var rVal;
var lVal;
var val;
var de;
var PositionedRight = false;

function onLoad(evt)
{
	comp = evt.getCurrentTarget();
	de=comp.getOwnerDocument().getDocumentElement();
	binds = comp.getElementsByTagName("Bind");
	swAni = comp.getChildByName("SwAni");
	bgAni = comp.getChildByName("BgAni");
	rVal = parseInt(comp.getAttribute("RightValue"));
	lVal = parseInt(comp.getAttribute("LeftValue"));
	de.setAttribute("requiredUnits", lVal);
}

function check(evt,val)
{
	theval = val;
	if (theval == rVal && !PositionedRight){
		animate(swAni, swAni.getAttribute("From"), swAni.getAttribute("To"));
		PositionedRight = true;
	}
	if (theval == lVal  && PositionedRight){
		animate(swAni, swAni.getAttribute("From"), swAni.getAttribute("To"));
		PositionedRight = false;
	}	
}

function animate(ani, t, f){
	ani.setAttribute("Animation", "Stop");
	ani.setAttribute("To", t);
	ani.setAttribute("From", f);
	ani.setAttribute("Animation", "Start");
}

function onClick(evt)
{
x=evt;
	t = swAni.getAttribute("From");
	f = swAni.getAttribute("To");
	if (t<f){
		de.setAttribute("requiredUnits", lVal);
				check(x,lVal);
	}
	else{
		de.setAttribute("requiredUnits", rVal);
					check(x,rVal);
	}
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Default Heating Setpoint").getAttribute("theval");
updateunits(x,val,"Default Heating Setpoint_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Heating Setpoint Limit").getAttribute("theval");
updateunits(x,val,"Heating Setpoint Limit_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Cooling Setpoint Limit").getAttribute("theval");
updateunits(x,val,"Cooling Setpoint Limit_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Standby Temperature Differential").getAttribute("theval");
updateunits(x,val,"Standby Temperature Differential_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Occupied Heat Setpoint").getAttribute("theval");
updateunits(x,val,"Occupied Heat Setpoint_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Standby Heat Setpoint").getAttribute("theval");
updateunits(x,val,"Standby Heat Setpoint_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Unoccupied Heat Setpoint").getAttribute("theval");
updateunits(x,val,"Unoccupied Heat Setpoint_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Occupied Cool Setpoint").getAttribute("theval");
updateunits(x,val,"Occupied Cool Setpoint_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Standby Cool Setpoint").getAttribute("theval");
updateunits(x,val,"Standby Cool Setpoint_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Unoccupied Cool Setpoint").getAttribute("theval");
updateunits(x,val,"Unoccupied Cool Setpoint_Display");
val=evt.getCurrentTarget().getOwnerDocument().getElementById("Room Temperature").getAttribute("theval");
updateunits(x,val,"Room Temperature_Display");
//now do the setpoint calc
	EffOcc=parseInt(comp.getOwnerDocument().getElementById("Setpoint").getAttribute("Effective Occupancy"));
	PiHDmd=parseInt(comp.getOwnerDocument().getElementById("Setpoint").getAttribute("PI Heating Demand"));
	PiCDmd=parseInt(comp.getOwnerDocument().getElementById("Setpoint").getAttribute("PI Cooling Demand"));
	sysMode=parseInt(comp.getOwnerDocument().getElementById("Setpoint").getAttribute("System Mode"));
	if(sysMode != 3 && PiCDmd == 0 && EffOcc == 1) evt.getCurrentTarget().getOwnerDocument().getElementById("Setpoint").setAttribute("Content",maths(evt,evt.getCurrentTarget().getOwnerDocument().getElementById("Occupied Heat Setpoint").getAttribute("theval")));
	if(sysMode != 3 && PiCDmd == 0 && EffOcc == 4) evt.getCurrentTarget().getOwnerDocument().getElementById("Setpoint").setAttribute("Content",maths(evt,evt.getCurrentTarget().getOwnerDocument().getElementById("Standby Heat Setpoint").getAttribute("theval")));
	if(sysMode != 3 && PiCDmd == 0 && EffOcc == 2) evt.getCurrentTarget().getOwnerDocument().getElementById("Setpoint").setAttribute("Content",maths(evt,evt.getCurrentTarget().getOwnerDocument().getElementById("Unoccupied Heat Setpoint").getAttribute("theval")));
	if(sysMode != 4 && PiHDmd == 0 && EffOcc == 1) evt.getCurrentTarget().getOwnerDocument().getElementById("Setpoint").setAttribute("Content",maths(evt,evt.getCurrentTarget().getOwnerDocument().getElementById("Occupied Cool Setpoint").getAttribute("theval")));
	if(sysMode != 4 && PiHDmd == 0 && EffOcc == 4) evt.getCurrentTarget().getOwnerDocument().getElementById("Setpoint").setAttribute("Content",maths(evt,evt.getCurrentTarget().getOwnerDocument().getElementById("Standby Cool Setpoint").getAttribute("theval")));
	if(sysMode != 4 && PiHDmd == 0 && EffOcc == 2) evt.getCurrentTarget().getOwnerDocument().getElementById("Setpoint").setAttribute("Content",maths(evt,evt.getCurrentTarget().getOwnerDocument().getElementById("Unoccupied Cool Setpoint").getAttribute("theval")));
}

function updateunits(evt,val,target)
{
		var str = "...";
				myloc=String(evt.getCurrentTarget().getAttribute("localeunits"));
				req=de.getAttribute("requiredUnits");
				if(myloc=="°C" && req=="1")
				{
			str=Number(val).toFixed(1);
				}
				if(myloc=="°C" && req=="2")
				{
			str=((parseFloat(val)*9)/5+32).toFixed(1);
				}
				if(myloc=="°F" && req=="1")
				{
			str=(((parseFloat(val)-32))*5/9).toFixed(1);
				}
				if(myloc=="°F" && req=="2")
				{
			str=Number(val).toFixed(1);
				}				
			if(req=="1") units="°C";
			if(req=="2") units="°F";		
	// write to the textbox		
//	return(str + " "+units);
evt.getCurrentTarget().getOwnerDocument().getElementById(target).setAttribute("Content",str+" "+units);
	}
	
	function maths(evt,val)
{
		var str = "...";
				myloc=String(evt.getCurrentTarget().getAttribute("localeunits"));
				req=de.getAttribute("requiredUnits");
				if(myloc=="°C" && req=="1")
				{
			str=Number(val).toFixed(1);
				}
				if(myloc=="°C" && req=="2")
				{
			str=((parseFloat(val)*9)/5+32).toFixed(1);
				}
				if(myloc=="°F" && req=="1")
				{
			str=(((parseFloat(val)-32))*5/9).toFixed(1);
				}
				if(myloc=="°F" && req=="2")
				{
			str=Number(val).toFixed(1);
				}				
			if(req=="1") units="°C";
			if(req=="2") units="°F";		
	// write to the textbox		
	return(str + " "+units);

	}
	
function onChange(evt)
{
evt.getCurrentTarget().setAttribute("localeunits",evt.getUnit());
}]]></Script>
