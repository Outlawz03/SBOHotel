                    <Script OnDocumentLoad="onLoad" OnMouseClick="" OnSignalChange="onChange"><![CDATA[var Units;
var myVal;
var ru;
var myunit;
function onLoad(evt)
{
ct=evt.getCurrentTarget();//setpoint ID/Name
de=ct.getOwnerDocument().getDocumentElement();
}

function onChange(evt)
{
	myUnit=String(evt.getUnit()).toLowerCase();
	if (myUnit.indexOf("°c") >=0) myunit="1";
	if (myUnit.indexOf("°f") >=0) myunit="2";
	ru=de.getAttribute("requiredUnits");
	Units=String(ct.getAttribute("Network Units"));
	OccCSP=parseFloat(ct.getAttribute("Occupied Cool Setpoint")).toFixed(1);
	OccHSP=parseFloat(ct.getAttribute("Occupied Heat Setpoint")).toFixed(1);
	UnOccCSP=parseFloat(ct.getAttribute("Unoccupied Cool Setpoint")).toFixed(1);
	UnOccHSP=parseFloat(ct.getAttribute("Unoccupied Heat Setpoint")).toFixed(1);
	SbyCSP=parseFloat(ct.getAttribute("Standby Cool Setpoint")).toFixed(1);
	SbyHSP=parseFloat(ct.getAttribute("Standby Heat Setpoint")).toFixed(1);
	EffOcc=parseInt(ct.getAttribute("Effective Occupancy"));
	PiHDmd=parseInt(ct.getAttribute("PI Heating Demand"));
	PiCDmd=parseInt(ct.getAttribute("PI Cooling Demand"));
	sysMode=parseInt(ct.getAttribute("System Mode"));
	if (de.getAttribute("requiredUnits")==1) units = "°C";	
	if (de.getAttribute("requiredUnits")==2) units = "°F";	
	if(sysMode != 3 && PiCDmd == 0 && EffOcc == 1) ct.setAttribute("Content",math(OccHSP)+units);
	if(sysMode != 3 && PiCDmd == 0 && EffOcc == 4) ct.setAttribute("Content",math(SbyHSP)+units);
	if(sysMode != 3 && PiCDmd == 0 && EffOcc == 2) ct.setAttribute("Content",math(UnOccHSP)+units);
	if(sysMode != 4 && PiHDmd == 0 && EffOcc == 1) ct.setAttribute("Content",math(OccCSP)+units);
	if(sysMode != 4 && PiHDmd == 0 && EffOcc == 4) ct.setAttribute("Content",math(SbyCSP)+units);
	if(sysMode != 4 && PiHDmd == 0 && EffOcc == 2) ct.setAttribute("Content",math(UnOccCSP)+units);
}

function math(num)
{
			if(myunit==1 && ru==1) value=parseFloat(num);
			if(myunit==1 && ru==2) value=(parseFloat(num)*9)/5+32;
			if(myunit==2 && ru==1) value=(parseFloat(num)-32)*5/9;
			if(myunit==2 && ru==2) value=parseFloat(num);
			return(value);
}]]></Script>
