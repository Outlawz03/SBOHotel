var PositionedRight = false;

function onLoad(evt)
{
	comp = evt.getCurrentTarget();
	binds = comp.getElementsByTagName("Bind");
	swAni = comp.getChildByName("SwAni");
	bgAni = comp.getChildByName("BgAni");
	rVal = parseInt(comp.getAttribute("RightValue"));
	lVal = parseInt(comp.getAttribute("LeftValue"));
}

function onClick(evt)
{
	t = swAni.getAttribute("From");
	f = swAni.getAttribute("To");
	if (t<f){
		setValue(binds.item(1).getFullBindName(), rVal);
		id1 = setTimeout("setleft()", "2500");
	}
	else{
		setValue(binds.item(1).getFullBindName(), lVal);
		id1 = setTimeout("setright()", "2500");
	}
}

function onChange(evt)
{
	val = parseInt(evt.getValue());
	if (val == rVal && !PositionedRight){
		animate(swAni, swAni.getAttribute("From"), swAni.getAttribute("To"));
		PositionedRight = true;
	}
	if (val == lVal  && PositionedRight){
		animate(swAni, swAni.getAttribute("From"), swAni.getAttribute("To"));
		PositionedRight = false;
	}	
}

function animate(ani, t, f){
	ani.setAttribute("Animation", "Stop");
	ani.setAttribute("To", t);
	ani.setAttribute("From", f);
	ani.setAttribute("Animation", "Start");
}

function setleft(){
		setValue(binds.item(1).getFullBindName(), lVal);
}

function setright(){
		setValue(binds.item(1).getFullBindName(), rVal);
}]]></Script>
