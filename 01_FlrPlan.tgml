/**==========================================================================================
 * Bind Object
 */
MYAPI.Bind = {
	init: function(node) {
		this.node = node;
		if(this.node) {
			this.fbn = this.node.getFullBindName();
		}
	},
	enableUpdates: function(){
		try {
			if(this.node) {
				this.node.setAttribute("DynamicUpdates", "Enable");
			}
		}
		catch(err) {
		}
	},
	disableUpdates: function() {
		try {
			if(this.node) {
				this.node.setAttribute("DynamicUpdates", "Disable");
			}
		}
		catch(err) {
		}
	},
	setUpdates: function(b) {
		try {
			if(this.node) {
				this.node.setAttribute("DynamicUpdates", b ? "Enable" : "Disable");
			}
		}
		catch(err) {
		}
	},
	displayProperties: function() {
		if(this.fbn) {
			invoke(this.fbn,"EditProperties");
		}
	},
	relinquish: function() {
		try {
			if(this.fbn) {
				setValue(this.fbn,null);	
			}
		}
		catch(err) {
		}
	},
	write: function(val) {
		try {
			if(this.fbn) {
				setValue(this.fbn, val);
			}
		}
		catch(err) {
		}
	}
};

/**==========================================================================================
 * Text Object
 */
MYAPI.Text = {
	init: function(node) {
		this.node = node;		
	},
	setContent: function(s) {
		if(this.node) {
			this.node.setAttribute("Content", s);
		}
	},
	setStroke: function(s) {
		if(this.node) {
			this.node.setAttribute("Stroke", s);
		}
	},
