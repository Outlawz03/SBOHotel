                    <Script Name="Occupied Cool Setpoint"
                        OnDocumentLoad="onLoad" OnMouseClick="onClick"
                        OnMouseDown="onDown" OnMouseOut="onOut"
                        OnMouseOver="onOver" OnMouseUp="onUp"
                            OnSignalChange="onChange"><![CDATA[var comp;
var myunit;
var de;
var units;
var inc = false;
var dec = false;
var myValue = new valueObject;
var valx;
var valy = new Number();

function onLoad(evt){
	comp = evt.getCurrentTarget();
	myValue.init();
	myValue.out();
}

function onChange(evt){
if(evt.getTarget().getFullBindName()=="Occupied Cool Setpoint" && inc==true)
{
if(Number(evt.getValue())>valx){
inc=false;
evt.getCurrentTarget().getChildByName("waitbox").setAttribute("Visibility","Hidden");
}
}
if(evt.getTarget().getFullBindName()=="Occupied Cool Setpoint" && dec==true)
{
if(Number(evt.getValue())<valx)
{
dec=false;
evt.getCurrentTarget().getChildByName("waitbox").setAttribute("Visibility","Hidden");
}
}
	var val = evt.getCurrentTarget().getAttribute("theval");
	de=evt.getTarget().getOwnerDocument().getDocumentElement();
	if (de.getAttribute("requiredUnits")==1) units = "°C";	
	if (de.getAttribute("requiredUnits")==2) units = "°F";	
	var unt = String(evt.getUnit()).toLowerCase();
	if (unt.indexOf("°c") >=0) myunit="1";
	if (unt.indexOf("°f") >=0) myunit="2";
	var	fbn = evt.getTarget().getFullBindName();
	myValue.changeValue(val,"Occupied Cool Setpoint");
}

function onClick(evt){
	x=evt;
	evt.preventDefault();
	var xPos = evt.getCurrentTargetX();
	var btn = parseInt(evt.getButton());
	myValue.clicked(btn,xPos,x);
}

function onDown(evt){
	var xPos = evt.getCurrentTargetX();
	var btn = parseInt(evt.getButton());
	myValue.down(btn,xPos);
}

function onUp(evt){
	myValue.up();
}

function onOut(evt){
	myValue.up();
	myValue.out();
}


function onOver(evt)
{
	if(inc==false && dec==false) myValue.over();
}

function valueObject(){
	var node;
	var valueBind;
	var value;
	var text;
	var decs;
	var increment;
	var minusBox;
	var plusBox;
	var minus;
	var plus;
	var btnLight;
	var btnDark;
	var onOverBox;
	var onOutBox;
	var highLimit;
	var lowLimit;
	var marker;
	
	this.init = function(){
		node = comp;
		
		//get binds
		var bindGroup = node.getChildByName("binds");		
		if(bindGroup != null){
			var bindList = bindGroup.getElementsByTagName("Bind");
					valueBind = "Occupied Cool Setpoint";
					writeBind = "Occupied Cool Setpoint RD";
		}
		
		//get display objects
		var displayGroup = node.getChildByName("display");
		if(displayGroup != null){
			var point = displayGroup.getChildByName("AnalogPoint");
			if(point != null){
				decs = point.getAttribute("Decimals");
				minusBox = point.getChildByName("minusBox");
				plusBox = point.getChildByName("plusBox");
				minus = point.getChildByName("minus");
				plus = point.getChildByName("plus");
				onOverBox = point.getChildByName("onOverBox");
				onOutBox = point.getChildByName("onOutBox");
				highLimit =	parseFloat(point.getAttribute("HighLimit"));
				lowLimit = parseFloat(point.getAttribute("LowLimit"));
				marker = point.getChildByName("marker");
				
				if(minusBox != null){
					btnLight = minusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(0).getAttribute("Color");
					btnDark = minusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(1).getAttribute("Color");
				}
				increment = parseFloat(point.getAttribute("Increment"));
				var display = point.getChildByName("display");			
				if(display != null){
					text = display.getChildByName("foreground");			
				}
			}
		}		
	}
	
	this.changeValue = function(val,fbn){
		if(fbn == "Occupied Cool Setpoint"){
			var ru=de.getAttribute("requiredUnits");
			if(myunit==1 && ru==1) value=parseFloat(val);
			if(myunit==1 && ru==2) value=(parseFloat(val)*9)/5+32;
			if(myunit==2 && ru==1) value=(parseFloat(val)-32)*5/9;
			if(myunit==2 && ru==2) value=parseFloat(val);
		}
		if (value <= highLimit && value >= lowLimit)
		{
			var per = (value-lowLimit)/(highLimit-lowLimit);
			marker.setAttribute("X1", (60*per)+20);
			marker.setAttribute("X2", (60*per)+20);
		}
		else{
			if (value > highLimit){
				marker.setAttribute("X1", 80);
				marker.setAttribute("X2", 80);
			}
			else{
				marker.setAttribute("X1", 20);
				marker.setAttribute("X2", 20);
			}
		}
		renderPoint();		
	}
	
	this.clicked = function(btn, xPos, evt){
	x=evt;
		if(btn === 0){
			if(xPos <= 20){
				dec = true;
				changeSpt(x);
			}else{
				if(xPos >= 80){
					inc = true;
					changeSpt(x);
				}else{
					invoke(writeBind,"EditProperties");
				}
			}
		}
	}
	
	this.down = function(btn,xPos){
		if(btn === 0){
			if(xPos <= 20){
				minusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(0).setAttribute("Color","#626469");
				minusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(1).setAttribute("Color","#626469");
				minus.setAttribute("Stroke","#FFFFFF");
			}
			if(xPos >= 80){
				plusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(0).setAttribute("Color","#626469");
				plusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(1).setAttribute("Color","#626469");
				plus.setAttribute("Stroke","#FFFFFF");
			}
		}
	}

	this.up = function(){
		minusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(0).setAttribute("Color",btnLight);
		minusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(1).setAttribute("Color",btnDark);
		plusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(0).setAttribute("Color",btnLight);
		plusBox.getElementsByTagName("LinearGradient").item(0).getElementsByTagName("GradientStop").item(1).setAttribute("Color",btnDark);
		minus.setAttribute("Stroke","#626469");
		plus.setAttribute("Stroke","#626469");
	}
	
	this.over = function(){
		minusBox.setAttribute("Visibility", "Visible");
		plusBox.setAttribute("Visibility", "Visible");
		minus.setAttribute("Visibility", "Visible");
		plus.setAttribute("Visibility", "Visible");
		marker.setAttribute("Visibility", "Visible");
		onOverBox.setAttribute("Visibility", "Visible");
		onOutBox.setAttribute("Visibility", "Hidden");		
	}
	
	this.out = function(){
		minusBox.setAttribute("Visibility", "Hidden");
		plusBox.setAttribute("Visibility", "Hidden");
		minus.setAttribute("Visibility", "Hidden");
		plus.setAttribute("Visibility", "Hidden");
		marker.setAttribute("Visibility", "Hidden");
		onOverBox.setAttribute("Visibility", "Hidden");
		onOutBox.setAttribute("Visibility", "Visible");
	}
	
	var changeSpt = function(evt){
			valx = Number(evt.getCurrentTarget().getAttribute("theval"));
		if(inc){
			if (valx < highLimit){
				if (valx < lowLimit){
					valy = lowLimit;
				}
				else{
				valy = valx + increment;
				}
			}
			else{
				valy = highLimit;
			}	

			setValue(writeBind,+valy);
		}
		if(dec){
			if (valx > lowLimit){
				if (valx > highLimit){
					valy = highLimit;
				}
				else{
					valy = valx - increment;
				}
			}
			else{
				valy = lowLimit;
			}
			setValue(writeBind,+valy);
		}
	evt.getCurrentTarget().getChildByName("waitbox").setAttribute("Visibility","Visible");
	}
	
	var renderPoint = function(){
		if(text != null){
			text.setAttribute("Content",getDisplayText());
		}
	}
	
	var getDisplayText = function(){
		try{		
			var str = "...";
			if(!isNaN(value) && value != Infinity){
				str = new Number(value).toFixed(decs);
				str += " " + units;
			}
			return str;
		}catch(ex){
			return "...";
		}
	}
	
	var getBool = function(val){
		if(val == "1" || val == "true" || val === 1 || val === true){
			return true;
		}
		return false;		
	}
}]]><Expose
