                        OnMouseUp="" OnSignalChange="onChange"><![CDATA[var background;
var oC;
var tC;
var txt;

function onLoad(evt)
{	
	background = evt.getCurrentTarget().getChildByName("background");
	txt = evt.getCurrentTarget().getChildByName("Text");
}

function onChange(evt)
{
b="#404040";
w="#FFFFFF";
lb=evt.getCurrentTarget().getAttribute("LowBattAlarm");
oc=evt.getCurrentTarget().getAttribute("OccCommand");
eo=evt.getCurrentTarget().getAttribute("EffectiveOcc");

if(lb == 1)
{
   background.setAttribute("Fill", "#FFD100");
   txt.setAttribute("Content", "Low Battery");
   txt.setAttribute("Stroke", b);
   return;
}


else if((oc == 1 || oc== 2) && eo == 1)
{
   background.setAttribute("Fill", "#339999");
   txt.setAttribute("Content", "Rented/Occupied");
      txt.setAttribute("Stroke", w);
   return;
}

else if((oc == 1 || oc== 2) && eo == 4)
{
   background.setAttribute("Fill", "#339999");
   txt.setAttribute("Content", "Rented/Standby");
      txt.setAttribute("Stroke", w);
   return;
}

else if((oc == 1 || oc== 2) && (eo == 2 || eo == 3))
{
   background.setAttribute("Fill", "#42B4E6");
   txt.setAttribute("Content", "Rented/Unoccupied");
      txt.setAttribute("Stroke", w);
   return;
}


else if(oc == 3 && eo == 1)
{
   background.setAttribute("Fill", "#4FA600");
   txt.setAttribute("Content", "Unrented/Occupied");
      txt.setAttribute("Stroke", w);
   return;
}

else if(oc == 3 && eo == 4)
{
   background.setAttribute("Fill", "#4FA600");
   txt.setAttribute("Content", "Unrented/Standby");
      txt.setAttribute("Stroke", w);
   return;
}

else if(oc == 3 && (eo == 2 || eo == 3))
   {
      background.setAttribute("Fill", "#87D300");
      txt.setAttribute("Content", "Unrented/Unoccupied");
         txt.setAttribute("Stroke", b);
      return;
   }
   
else
{
   background.setAttribute("Fill", "#FF0000");
   txt.setAttribute("Content", "No Communication");
      txt.setAttribute("Stroke", w);
}
//end
}]]></Script>
