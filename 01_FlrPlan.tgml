    <Script Name="GS" OnDocumentLoad="load" OnMouseClick="click"
        OnMouseDown="" OnMouseMove="" OnMouseOut="" OnMouseOver=""
        OnMouseUp="" OnSignalChange="change"><![CDATA[//NameSpace
global.TGMLAPI = {}

var MYAPI = global.TGMLAPI;
var _ = global._;


/**==========================================================================================
 * Helper Functions
 */
MYAPI.isUndefined = function(o) {
	if(typeof(o) === "undefined") {
		return true;			
	}
	return false;	
}

MYAPI.getBool = function(s) {
	var trueEnum = ["yes", "y", "Yes", "Y", "1", "True", "true", "T", "t", "On", "active"];
	var i = trueEnum.length;
	while(i--) {
		if(String(s) === trueEnum[i]) {
			return true;
		}
	}
	return false
}

if (!Object.create) {
	Object.create = function(base) {
		function F () {};
		F.prototype = base;
		return new F();
	};
}

/**==========================================================================================
 * Main Object
 */
var Main = {		
	load: function(e) {
		//get filter buttons
		var TGMLDoc = e.getCurrentTarget().getOwnerDocument(),
			TGMLElem = TGMLDoc.getDocumentElement(),
			display = TGMLElem.getChildByName("Display"),
			binds = TGMLElem.getChildByName("Binds");
		
		// Create Display Manager
		this.dm = Object.create(MYAPI.DisplayManager);
		this.dm.init(display);
		
		// Create Bind Manager
		this.bm = Object.create(MYAPI.BindManager);
		this.bm.init(binds);				
	},
	
	// change event handler
	change: function(e) {
		e.preventDefault();
		this.dm.update(e);
		this.bm.update(e);		
	},
	
	// mouse click event handler
	click: function(e) {
		this.dm.click(e);
		this.bm.click(e);
	}
};

/**==========================================================================================
 * Display Manager Object
 */
MYAPI.DisplayManager = {
	per: 25,
	init: function(node) {
		// get containers
		this.containers = [];
		var rcView = node.getChildByName("RCView");
		
		this.canvasWidth = +rcView.getAttribute("Width");
		this.canvasHeight = +rcView.getAttribute("Height");
			
		this.activeContainer = 0;	
		
		var containerList = rcView.getChildNodes();
		this.containers = _.map(containerList, function(){return Object.create(MYAPI.Container);});
		_.each(this.containers, function(node, index){node.init(containerList.item(index));});
		
		this.lower = node.getChild("Lower");
		this.higher = node.getChild("Higher");
		
		this.lower.setAttribute("Visibility", "Hidden");
		this.higher.setAttribute("Visibility", this.containers.length > 1 ? "Visible" : "Hidden");
		
		// get Template
		this.template = node.getChildByName("RoomController");
		
		// get Filters
		
		// get SE8000
		
		this.rcs = {};		
	},
	
	update: function(e) {
		var RCName = String(e.getTarget().getParentNode().getAttribute("Name"));
		
		if(RCName.slice(0,2) === "RC") {
			// check if the RC exists
			var rcNum = +RCName.slice(-2);
			if(!_.has(this.rcs, rcNum)) {
				this.rcs[rcNum] = Object.create(MYAPI.RCObject);					
				this.rcs[rcNum].init({	model: this.template, 
										num: rcNum,
										perScreen: this.per,
										parent: this.containers[Math.floor((rcNum - 1) / this.per)].node, 
										canvasWidth: this.canvasWidth,
										canvasHeight: this.canvasHeight
									});
			}
			
			// update rc display
			this.rcs[rcNum].update(e);
		}
	},
	
	click: function(e) {
		var name = String(e.getTarget().getAttribute("Name"));
		
		if(name === "Lower" || name === "Higher") {
			if(name === "Lower") {
				this.activeContainer = Math.max(this.activeContainer - 1, 0);	
			} else if(name === "Higher") {
				this.activeContainer = Math.min(this.activeContainer + 1, this.containers.length - 1);				
			}		
			
			this.lower.setAttribute("Visibility", this.activeContainer === 0 ? "Hidden" : "Visible");
			this.higher.setAttribute("Visibility", this.activeContainer === (this.containers.length - 1) ? "Hidden" : "Visible");
			
			for(var i = 0, max = this.containers.length; i < max; i += 1) {
				this.containers[i].update(false);
			}
			this.containers[this.activeContainer].update(true);
		}
	}
};

/**==========================================================================================
 * RCObject Object
 */
MYAPI.RCObject = {
	init: function(props) {
		// create clone
		this.node = props.model.cloneNode(true);
		
		// attach to correct container
		props.parent.appendChild(this.node);
		
		//set RC Name for click events
		this.node.setAttribute("Name", "RC_" + (props.num < 10 ? "0" : "") + props.num);
		
		// make clone visible
		this.node.setAttribute("Visibility", "Visible");
		
		this.screenNum = props.num - (Math.floor((props.num - 1) / props.perScreen) * props.perScreen);		
		
		// position clone
		var width = +this.node.getAttribute("Width"),
			height = +this.node.getAttribute("Height"),
			col = Math.floor(Math.sqrt(props.perScreen)),
			row = Math.floor(props.perScreen / col),
			colWidth = props.canvasWidth / col,
			rowHeight = props.canvasHeight / row,
			cellRow = Math.floor((this.screenNum - 1) / col),
			cellColumn = (this.screenNum - 1) % col;
			cellTop = cellRow * rowHeight,
			cellLeft = cellColumn * colWidth;
		
		this.node.setAttribute("Top", cellTop + (rowHeight - height) / 2);
		this.node.setAttribute("Left", cellLeft + (colWidth - width) / 2);
		
		this.widgets = {};
		var children = this.node.getChildNodes();
		for(var i = 0, max = children.length; i < max; i += 1) {
			var child = children.item(i);
			if(child.hasAttribute("Model")) {
				var model = String(child.getAttribute("Model")),
					ctrl = String(child.getAttribute("Ctrl"));
				this.widgets[model] = Object.create(eval("MYAPI." + ctrl));
				this.widgets[model].init(child);
			}
		}
	},
	update: function(e) {
		var name = String(e.getTarget().getAttribute("Name"));		
		if(_.has(this.widgets, name)) {
			this.widgets[name].update(e);
		}
		
		this.widgets["Setpoint"].update(e);
		this.widgets["Occupancy"].update(e);
		this.widgets["Header"].update(e);
	}	
};

/**==========================================================================================
 * Container Object
 */
MYAPI.Container = {
	init: function(node) {
		this.node = node;
		this.visible = false;
	},
	update: function(b) {
		this.visible = b;
		this.node.setAttribute("Visibility", this.visible ? "Visible" : "Hidden");
	}
};

/**==========================================================================================
 * Bind Manager Object
 */
