/**==========================================================================================
 * Simple Analog Value Object
 */
MYAPI.AnalogValue = {
	init: function(node) {
		this.node = node;
				
		this.decimals = 1;
		if(this.node.hasAttribute("Decimals")) {
			this.decimals = +this.node.getAttribute("Decimals");
		}
	},
	
	update: function(e) {
		var value = +e.getValue();
		var unit = e.getUnit();
		
		this.node.setAttribute("Content", value.toFixed(this.decimals) + " " + unit);
	}
};

/**==========================================================================================
 * System Mode Control
 */
MYAPI.SystemModeCtrl = {
	init: function(node) {
		var children = node.getChildNodes();
		this.icons = _.map(children, function(){return Object.create(MYAPI.Component);});
		_.each(this.icons, function(obj, i){obj.init(children.item(i));});
	},
	
	update: function(e) {
		var value = +e.getValue();
		_.each(this.icons, function(obj, i){obj.setVisibility(false);});
		if(_.has(this.icons, value - 1)) {
			this.icons[value - 1].setVisibility(true);
		}
	}
};

/**==========================================================================================
 * Fan Mode Control
 */
MYAPI.FanModeCtrl = {
	init: function(node) {
		this.txt = Object.create(MYAPI.Text);
		this.txt.init(node.getChild("autoTxt"));
		var children = node.getChild("Speeds").getChildNodes();
		
		this.spds = _.map(children, function(){return Object.create(MYAPI.Pie);});
		_.each(this.spds, function(obj, i){obj.init(children.item(i));});
	},
	
	update: function(e) {
		var value = +e.getValue();
		
		_.each(this.spds, function(node, i){node.setFill(value > i ? "#626469" : "#9FA0A4");});
		
		this.txt.setVisibility(value > 3 ? true : false);
		this.txt.setContent(value === 5 ? "On" : "Auto");
	}
};

/**==========================================================================================
 * NOTE1 Control
 */
MYAPI.NOTE1Ctrl = {
	init: function(node) {
		this.txt = Object.create(MYAPI.Text);
		this.txt.init(node);
	},
	
	update: function(e) {
		var content = String(e.getValue());
		if(content === "") {
			content = "Click Here";
		}
		this.txt.setContent(content);
	}
};

/**==========================================================================================
 * Event Callback Functions
 */

function load(evt){	
	Main.load(evt);
}

function change(evt) {	
	Main.change(evt);
}

function click(evt) {
	Main.click(evt);	
}]]></Script>
